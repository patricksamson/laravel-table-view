{
    "name": "lykegenes/laravel-table-view",
    "description": "An API-based table view for Laravel. Based on Vue.js and Element-UI.",
    "keywords": [
        "laravel",
        "datatable"
    ],
    "homepage": "https://github.com/Lykegenes/laravel-table-view",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Samson",
            "email": "patricksamson236@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.2|~6.0|~7.0|~8.0|~9.0",
        "prettus/l5-repository": "~2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.5",
        "orchestra/testbench": "~3.0|~4.0|~5.0|~6.0|~7.0"
    },
    "autoload": {
        "psr-4": {
            "Lykegenes\\TableView\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lykegenes\\TableView\\Providers\\TableViewServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
